<Window x:Class="HardAnalyzeSys.ActionForm.DataPreprocForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HardAnalyzeSys.ActionForm"
        mc:Ignorable="d"
        Title="Предобработка данных" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lb_titles" HorizontalAlignment="Left" Height="357" Margin="10,52,0,0" VerticalAlignment="Top" Width="245" SelectionMode="Multiple"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Выберите параметры для создания выборки:" VerticalAlignment="Top" Height="42" Width="153"/>
        <CheckBox x:Name="choose_all" Content="Выбрать все" HorizontalAlignment="Left" Margin="168,22,0,0" VerticalAlignment="Top" Checked="choose_all_Checked" Unchecked="choose_all_Unchecked"/>

    </Grid>
</Window>
